{
    "name": "mquinn/box-api-sdk",
    "description": "This is an unofficial Box.com API SDK.",
    "type": "library",
    "authors": [
        {
            "name": "Mitchell Quinn",
            "email": "mitchell.david.quinn@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "ext-json": "*",
        "ext-openssl": "*"
    },
    "autoload": {
        "psr-4": {
            "Mitquinn\\BoxApiSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mitquinn\\BoxApiSdk\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "symfony/dotenv": "^5.1",
        "firebase/php-jwt": "^5.2"
    }
}
